# CDK Configuration Environment Variables
# Copy this file to .env and customize the values

# Resource naming prefix (default: solr2os)
NAME_PREFIX=solr2os

# OpenSearch domain name (default: {namePrefix}-domain)
DOMAIN_NAME=my-opensearch-domain

# OpenSearch index name (default: solr-migration)
INDEX_NAME=my-index

# VPC CIDR block (default: 10.0.0.0/16)
VPC_CIDR=10.0.0.0/16

# ENABLE_PIPELINE
ENABLE_PIPELINE=false
